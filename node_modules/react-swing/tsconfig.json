{
  "compilerOptions": {
    "lib": ["es2017", "dom"],
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmitOnError": true,
    "outDir": "dist/",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "target": "es5",
    "jsx": "react"
  },
  "include": ["./src/**/*"]
}
